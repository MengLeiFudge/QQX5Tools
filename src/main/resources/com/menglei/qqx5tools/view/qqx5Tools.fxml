<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="root" prefHeight="618.0" prefWidth="1000.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.menglei.qqx5tools.controller.QQX5ToolsController">
    <center>
        <VBox BorderPane.alignment="CENTER">
            <GridPane alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="382.0" prefWidth="1000.0">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="430.0" minWidth="10.0" prefWidth="430.0" />
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES" />
                </rowConstraints>
                <Button fx:id="btn_calculateBurstPoint" mnemonicParsing="false" onAction="#gotoCalculateBurstPoints1" prefHeight="36.0" prefWidth="230.0" text="计算爆点" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
                <Button fx:id="btn_openBurstPointsInfoDir" mnemonicParsing="false" onAction="#openBurstPointsInfoDir" prefHeight="36.0" prefWidth="230.0" text="查看爆点" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
                <Button fx:id="btn_adjustBpm1" mnemonicParsing="false" onAction="#gotoAdjustBpm1" prefHeight="36.0" prefWidth="230.0" text="调整谱面bpm" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
                <Button fx:id="btn_uploadXml" mnemonicParsing="false" onAction="#gotoUploadXml1" prefHeight="36.0" prefWidth="230.0" text="上传自制谱" GridPane.halignment="CENTER" GridPane.rowIndex="3" GridPane.valignment="CENTER" />
                <Button fx:id="btn_bytesToXml1" mnemonicParsing="false" onAction="#gotoBytesToXml1" prefHeight="36.0" prefWidth="230.0" text="bytes转xml" GridPane.halignment="CENTER" GridPane.rowIndex="4" GridPane.valignment="CENTER" />
                <Button fx:id="btn_downloadNotRankSongs" mnemonicParsing="false" onAction="#gotoDownloadNotRankSongs" prefHeight="36.0" prefWidth="230.0" text="下载非排位歌曲并解压" GridPane.halignment="CENTER" GridPane.rowIndex="5" GridPane.valignment="CENTER" />
            </GridPane>
        </VBox>
    </center>
    <bottom>
        <GridPane prefHeight="100.0" BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="300.0" prefWidth="300.0" />
                <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="300.0" prefWidth="300.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="90.0" prefHeight="90.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <Button fx:id="btn_help" mnemonicParsing="false" onAction="#gotoHelp" prefHeight="36.0" prefWidth="144.0" text="帮助" GridPane.halignment="RIGHT" GridPane.hgrow="NEVER" GridPane.valignment="TOP" GridPane.vgrow="NEVER" />
            <Button fx:id="btn_exit" mnemonicParsing="false" onAction="#exit" prefHeight="36.0" prefWidth="144.0" text="退出" textAlignment="CENTER" GridPane.columnIndex="2" GridPane.halignment="LEFT" GridPane.hgrow="NEVER" GridPane.valignment="TOP" GridPane.vgrow="NEVER" />
        </GridPane>
    </bottom>
    <top>
        <Pane prefHeight="100.0" BorderPane.alignment="CENTER" />
    </top>
</BorderPane>
